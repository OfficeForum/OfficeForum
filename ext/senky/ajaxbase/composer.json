{
    "name": "senky/ajaxbase",
    "type": "phpbb-extension",
    "description": "This extension will ajaxify your phpBB board. Ajaxifies Who is online, Statistics and Post preview!",
    "version": "1.0.0",
    "time": "2015-07-30",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Jakub Senko",
            "email": "jakubsenko@gmail.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "Ajax Base",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.*"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/ajax_base",
            "filename": "version_check"
        }
    }
}